{
  "name": "login-api-with-ia",
  "version": "1.0.0",
  "description": "RESTful Login API with JavaScript, Mocha, and Supertest",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "set NODE_ENV=test&& mocha --timeout 10000 --reporter mochawesome --exit",
    "test:watch": "set NODE_ENV=test&& mocha --watch --timeout 5000",
    "test:coverage": "set NODE_ENV=test&& nyc mocha --timeout 10000 --reporter mochawesome --exit"
  },
  "keywords": [
    "api",
    "login",
    "authentication",
    "rest",
    "javascript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "chai": "^4.3.10",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "nodemon": "^3.0.2",
    "nyc": "^17.1.0",
    "supertest": "^6.3.3"
  }
}
